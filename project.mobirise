              "z-index": "1000",
              "padding": "0 1rem",
              "height": "80px"
            },
            ".navbar-brand .navbar-logo when (@showLogo)": {
              "amp-img, img": {
                "height": "~\"@{logoHeight}px\"",
                "width": "~\"@{logoWidth}px\""
              }
            },
            ".menu-container": {
              "min-height": "100%"
            },
            "& when (@showBrand)": {
              ".navbar-caption": {
                "line-height": "inherit !important"
              }
            },
            ".navbar .navbar-collapse": {
              "@media (max-width: 991px)": {
                "background": "@menuBgColor",
                "& when (@sidebarSide = 'left')": {
                  "right": "auto",
                  "left": "0"
                }
              }
            },
            "& when (@showItems)": {
              ".nav-link": {
                "margin": ".667em 1em !important",
                "padding": "0 !important"
              },
              ".dropdown-menu": {
                "background": "@menuBgColor"
              },
              ".dropdown-item": {
                "position": "relative",
                "&:before": {
                  "content": "\"\"",
                  "position": "absolute",
                  "left": "-15px",
                  "top": "50%",
                  "transform": "translateY(-50%) rotate(45deg)",
                  "opacity": "0",
                  "margin-right": "15px",
                  "width": "5px",
                  "height": "5px"
                },
                "&,&:before": {
                  "transition": "all .3s"
                }
              }
            },
            ".dropdown-item": {
              "justify-content": "flex-start !important",
              "&:active, &:focus, &:hover": {
                "background": "@menuBgColor",
                "color": "currentColor"
              }
            },
