            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              "& when (@overlay)": {
                ".mbr-overlay": {
                  "background": "@overlayColor",
                  "opacity": "@overlayOpacity"
                }
              }
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            ".icon-box": {
              "@media (max-width: 767px)": {
                "padding-left": "1rem",
                "padding-right": "1rem"
              }
            },
            "align-items": "center",
            "display": "flex",
            ".btn": {
