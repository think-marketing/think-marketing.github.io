                ".amp-iconfont": {
                  "color": "contrast(@icon3Bg)"
                }
              }
            },
            ".custom-bg-4": {
              "transition": "0.5s",
              "&:hover": {
                "background-color": "@icon4Bg",
                ".amp-iconfont": {
