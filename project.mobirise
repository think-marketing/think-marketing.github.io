          "_name": "ampFeatures20",
          "_customHTML": "<section class=\"ampFeatures20\" group=\"Features\" plugins=\"amp-main\">\n    <mbr-parameters>\n        <header>Size:</header>\n        <input type=\"range\" inline title=\"Top:\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Bottom:\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n        <header>Cards:</header>\n        <input type=\"range\" inline title=\"Cards:\" name=\"amountCards\" min=\"1\" max=\"3\" step=\"1\" value=\"1\">\n        <select title=\"Align:\" name=\"cardsAlign\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected>Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Title:\" name=\"showCardsTitle\" checked>\n        <input type=\"checkbox\" title=\"Icon:\" name=\"checkIcons\" checked>    \n        <input type=\"range\" inline title=\"Size Icon:\" name=\"iconsSize\" min=\"1\" max=\"8\" step=\"0.5\" value=\"3\" condition=\"checkIcons\">\n        <input type=\"color\" title=\"Color Icon:\" name=\"colorIcon\" value=\"#7567f8\" condition=\"checkIcons\">\n        <!--  -->\n        <header>Background:</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background6.jpg\" selected>\n        </fieldset>\n        <!--  -->\n        <input type=\"checkbox\" title=\"Overlay:\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color:\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity:\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <!--  -->\n    <!-- Overlay -->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <!-- Container -->\n    <div class=\"container\">\n        <!-- Content -->\n        <div class=\"mbr-row mbr-jc-c wrapper-content\" mbr-if=\"showCardsTitle || checkIcons\">\n            <!-- Card-1 -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-4 p-0\">\n                <div class=\"card-wrapper mbr-flex mbr-py-2\">\n                    <div class=\"card card1\">\n                        <div class=\"card-img align-center mbr-pb-4\" mbr-if=\"checkIcons\">\n                            <span class=\"iconfont-wrapper\">\n                                <span mbr-icon class=\"amp-iconfont icon fa-google fa\"></span>\n                            </span>\n                        </div>\n                        <div class=\"card-box mbr-px-2\" mbr-if=\"showCardsTitle\" mbr-class=\"{'align-left' : cardsAlign == 1, 'align-center': cardsAlign == 2, 'align-right': cardsAlign == 3, 'mbr-py-2' : !checkIcons}\">\n                            <h5 class=\"card-subtitle mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-subtitle\"></h5>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <!-- Card-2 -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-4 p-0\" mbr-if=\"amountCards > 1\">\n                <div class=\"card-wrapper mbr-flex mbr-py-2\">\n                    <div class=\"card card2\">\n                        <div class=\"card-img align-center mbr-pb-4\" mbr-if=\"checkIcons\">\n                            <span class=\"iconfont-wrapper\">\n                                <span mbr-icon class=\"amp-iconfont icon fa-heart fa\"></span>\n                            </span>\n                        </div>\n                        <div class=\"card-box mbr-px-2\" mbr-if=\"showCardsTitle\" mbr-class=\"{'align-left' : cardsAlign == 1, 'align-center': cardsAlign == 2, 'align-right': cardsAlign == 3, 'mbr-py-2' : !checkIcons}\">\n                            <h5 class=\"card-subtitle mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-subtitle\">CREATIVE DESIGN</h5>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <!-- Card-3 -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-4 p-0\" mbr-if=\"amountCards > 2\">\n                <div class=\"card-wrapper mbr-flex mbr-py-2\">\n                    <div class=\"card card3\">\n                        <div class=\"card-img align-center mbr-pb-4\" mbr-if=\"checkIcons\">\n                            <span class=\"iconfont-wrapper\">\n                                <span mbr-icon class=\"amp-iconfont icon fa-google fa\"></span>\n                            </span>\n                        </div>\n                        <div class=\"card-box mbr-px-2\" mbr-if=\"showCardsTitle\" mbr-class=\"{'align-left' : cardsAlign == 1, 'align-center': cardsAlign == 2, 'align-right': cardsAlign == 3, 'mbr-py-2' : !checkIcons}\">\n                            <h5 class=\"card-subtitle mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-subtitle\">BRANDING</h5>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
